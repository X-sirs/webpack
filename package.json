{
  "name": "mygulpack",
  "version": "1.0.0",
  "description": "my test app",
  "scripts": {
    "test": "test",
    "dev": "better-npm-run build-dev",
    "build":"better-npm-run build-production",
    "server-dev": "npm-run-all --sequential build-dev start-server",
    "build-dev": "better-npm-run start-dev",
    "start-server": "better-npm-run build-server",
    "build:prod": "webpack --config=./webpack.config.prod.js",
    "lintjs": "eslint src/ webpack.*.js --cache",
    "dll": "webpack --config webpack.dll.config.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "betterScripts": {
    "build-dev": {
      "command": "gulp clean && webpack-dev-server  --config ./build/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-production":{
      "command": "gulp clean && webpack --colors --progress  --config ./build/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "gulp clean && webpack --colors --progress --config ./build/webpack.config.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-server": {
      "command": "nodemon ./build/dispatch.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "history": "^4.7.2",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-webpack-middleware": "^1.0.7",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "autoprefixer": "^7.2.6",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.6.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.6",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^0.5.9",
    "web-webpack-plugin": "^1.10.2",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.2",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "keywords": [
    "webpack"
  ],
  "author": "xxf",
  "license": "ISC"
}
